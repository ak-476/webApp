        methods:{
            init: function(){
                var that = this;
                that.fromHome = api.pageParam.fromHome;
                Util.post(Config.api.appGanoread, {}, function(data) {
                    that.countQaNoRead = data || 0;
                }, function () {}, true);
            },
            fault: function(option){
                if(!option){
                    if(!this.keyword){
                        Util.auiShowMsgBox({
                            msg: '搜索内容不能为空'
                        });
                        return;
                    }
                }
                api.openWin({
                    name: 'externalPage',
                    url: './common/header.html',
                    pageParam: {
                        file: 'table',
                        id: 1,
                        title: '你好',
                        url: Config.resourcePath.hostWx + 'v2/faultCode/list?keyword=' + this.keyword
                    }
                });
            },
            paseInfo: function(){
                if(!this.keyword){
                    Util.auiShowMsgBox({
                        msg: '搜索内容不能为空'
                    });
                    return;
                }

                api.openWin({
