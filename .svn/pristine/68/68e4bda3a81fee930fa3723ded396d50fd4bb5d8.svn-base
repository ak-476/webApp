                    url: './common/header.html',
                    pageParam: {
                        file: 'table',
                        id: 1,
                        title: '你好',
                        url: Config.resourcePath.hostWx + 'v2/universalSoso/searchList?keyword=' + this.keyword,
                    }
                });
            },
            datum: function () {
                api.openWin({
                    name: 'externalPage',
                    url: './common/header.html',
                    pageParam: {
                        file: 'table',
