                    }
                });
            },
            faultCodes: function () {
                api.openWin({
                    name: 'externalPage',
                    url: './common/header.html',
                    pageParam: {
                        file: 'table',
                        id: 1,
                        title: '你好',
                        url: Config.resourcePath.hostWx + 'v2/faultCode/index',
                    }
                });
            },
            online: function () {
                api.openWin({
                    name: 'externalPage',
                    url: './common/header.html',
                    pageParam: {
                        file: 'table',
                        id: 1,
                        title: '你好',
                        url: Config.resourcePath.hostApp + 'wxauth/onlineQA.htm?status=5',
                    }
                });
            },
            faultFind: function () {
                api.openWin({
                    name: 'externalPage',
                    url: './common/header.html',
                    pageParam: {
                        file: 'table',
                        id: 1,
                        title: '你好',
                        url: Config.resourcePath.hostApp + 'wxauth/commonRailForumList.htm?termId=81',
                    }
                });
            },

            inputClose: function () {
            	if (api.pageParam.fromHome) {
            		api.closeWin();
                } else {
            		this.keyword = '';
                }
            }
        }
    })

