	apiready = function () {
		var header = document.querySelector('#header');
		$api.fixIos7Bar(header);

		api.addEventListener({
			name: 'pageTitle'
		}, function(ret, err) {
			$api.text(document.getElementById('title'), ret.value.name);
		});

        api.addEventListener({
            name: 'keyback'
        }, function(ret, err) {
            back();
        });

		api.openFrame({
			name: 'baidu',
			url: api.pageParam.url,
			bounces: false,
			rect: {
				x: 0,
			    y: $api.dom('header').offsetHeight,
				//y: 70,
				w: 'auto',
				h: 'auto'
			}
		});
	};
</script>