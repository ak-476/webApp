<script type="text/javascript" src="../../script/api.js"></script>
<script type="text/javascript">
	function back() {
		api.historyBack({
			frameName: 'baidu'
		}, function (ret, err) {
			if (!ret.status) {
				api.closeWin();
			}
		});
	}

//	var getWebUrl = api.require('getWebUrl');
