                    });
                    return;
                }

                api.openWin({
                    name: 'externalPage',
                    url: './common/header.html',
                    pageParam: {
                        file: 'table',
                        id: 1,
                        title: '你好',
                        url: HOSTWX + '/v2/faultCode/list?keyword=' + this.keyword,
                    }
                })

//                api.openWin({
